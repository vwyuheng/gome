<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="cpn_coupon_batch">
	<resultMap id="cpnCouponBatchCatResult" class="com.tuan.pmt.dao.data.CpnCouponBatchCatDO">
		<result column="id" property="id" jdbcType="BIGINT" />
		<result column="batch_id" property="batchId" jdbcType="BIGINT" />
		<result column="cat_id" property="catId" jdbcType="INTEGER" />
	</resultMap>
	
	<select id="queryCpnCouponBatchCatByBatchIdAndCatId" parameterClass="java.util.Map" resultMap="cpnCouponBatchCatResult">
		select id,batch_id,cat_id 
		from cpn_coupon_batch_cat 
		where batch_id = #batchId:BIGINT# and cat_id = #catId:BIGINT#
	</select>
	
	<select id="queryCpnCouponBatchCatByBatchId" parameterClass="java.lang.Long" resultMap="cpnCouponBatchCatResult">
		select id,batch_id,cat_id 
		from cpn_coupon_batch_cat 
		where batch_id = #batchId:BIGINT# 
	</select>

</sqlMap>